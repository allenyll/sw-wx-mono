}

.order-list .a-order .order-date .red {
  font-size: 26rpx;
  color: #e64340;
}

.deal-tip {
  float: left;
  display: flex;
  line-height: 80rpx;
}

.deal-tip .title {
  font-size:20rpx; 
  color: #999;
  line-height: 83rpx;
}

.van-btn button{
  color: #999;
}

.goods-item {
  background-color: #fff;
  margin-top: 20rpx;
