.stv-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.withAnimate {
  transition: all 100ms ease;
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -ms-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-perspective: 1000;
  -moz-perspective: 1000;
  -ms-perspective: 1000;
  perspective: 1000;
}

.message__badge {
  position: absolute;
  top: -22rpx;
  right: 20rpx;
  z-index: 100;
}

.stv-container .tab-bar {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  justify-content: space-between;
  background-color: #fff;
  display: flex;
  font-size: 28rpx;
  color: #353535;
}

.stv-container .tab-bar .tab {
  height: 100%;
  text-align: center;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.stv-container .tab-bar .tab.active {
  color: #0387FE;
  font-weight: bold;
}

.stv-container .tab-bar .tab .red-dot {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  left: 116rpx;
  top: 23rpx;
  background-color: #0387FE;
  border-radius: 50%;
  font-size: 18;
  color: #000;
  text-align: center;
}

.stv-container .tab-bar .under-line {
  position: absolute;
  bottom: 0;
  height: 6rpx;
  background-color: #0387FE;
}

.stv-container .scroll-view {
  position: relative;
  width: 100%;
  height: 93%;
  background: #f2f2f2;
}

.stv-container .scroll-view .scroll-view-wrapper {
  position: absolute;
  top: 0;
  bottom: 0;
  display: flex;
}

.stv-container .scroll-view .scroll-view-wrapper .one-scene {
  overflow: visible;
  border-radius: 50%;
  height: 100%;
}

.no-order {
  width: 100%;
  height: 100%;
  padding-top: 100rpx;
  text-align: center;
  background-color: #f2f2f2;
}

.order-list {
  width: 100%;
}

.order-list .an-order {
  width: 95%;
  margin: 10rpx auto;
  border-radius: 20rpx;
  background-color: #fff;
}

.order-list .an-order-hd{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  font-size: 28rpx;
}

.order-list .an-order-hd .an-order-icon{
  float: left;
  font-size: 36rpx;
  height: 60rpx;
  line-height: 70rpx;
}

.order-detail {
  border-top: 2rpx solid #eee;
  margin: 0 20rpx;
}

.van-card__title {
  font-size: 26rpx;
  margin-bottom: 10rpx;
}

.order-list .a-order {
  width: 94%;
  background-color: #fff;
  margin:20rpx auto;
  border-radius:20rpx;
}

.order-list .a-order .order-date {
  width: 92%;
  margin: 0 auto;
  /* padding: 0 30rpx; */
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  color: #000;
